{
  "name": "bananaquitjs",
  "version": "0.1.3",
  "description": "Bananaquit is a small and simple component framework using TypeScript/JavaScript.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgrassl/bananaquit.git"
  },
  "keywords": [
    "typescript",
    "component"
  ],
  "author": "Timon Grassl <grassl.timon@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgrassl/bananaquit/issues"
  },
  "homepage": "https://github.com/tgrassl/bananaquit#readme",
  "files": ["dist/*"],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "watch": "microbundle watch",
    "build": "microbundle",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "postpublish": "npm run clean",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "bumpVersion": "npm version patch"
  },
  "dependencies": {
    "@types/node": "^12.7.4",
    "chokidar": "^3.0.2",
    "fs-extra": "^8.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  }
}
